{
  "name": "princemuel",
  "type": "module",
  "version": "0.3.0",
  "description": "My personal website built with Astro, Qwik, Tailwind CSS, MDX, and the Github API",
  "private": true,
  "repository": "git@github.com:princemuel/princemuel.com.git",
  "author": "princemuel <vansomecsam@gmail.com>",
  "scripts": {
    "dev": "astro dev --host",
    "start": "astro dev",
    "serve": "netlify dev --listen 4321",
    "build": "bash ./scripts/unshallow.sh && astro build",
    "build:prod": "astro check && astro build",
    "build:force": "astro check && astro build -- --force",
    "preview": "astro preview",
    "astro": "astro",
    "typecheck": "tsc --noEmit",
    "lint": "tsc --noEmit -p tsconfig.json && biome lint --diagnostic-level=error --no-errors-on-unmatched .",
    "lint:fix": "biome check --write --unsafe .",
    "lint:staged": "biome check --error-on-warnings --no-errors-on-unmatched --staged .",
    "db:start": "supabase start",
    "db:status": "supabase status",
    "db:stop": "supabase stop",
    "db:link": "supabase link",
    "db:reset": "prisma db push",
    "db:introspect": "prisma db pull",
    "db:gen": "prisma generate",
    "db:sql": "prisma generate --sql --watch",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@libsql/client": "^0.14.0",
    "@netlify/edge-functions": "^2.11.0",
    "@netlify/functions": "^2.8.2",
    "@prisma/adapter-libsql": "^5.22.0",
    "@prisma/client": "^5.22.0",
    "@upstash/ratelimit": "^2.0.4",
    "@upstash/redis": "^1.34.3",
    "isbot": "^5.1.17",
    "octokit": "^4.0.2",
    "outvariant": "^1.4.3",
    "resend": "4.0.1-alpha.0",
    "sharp": "^0.33.5",
    "signal-polyfill": "^0.2.1",
    "tailwind-variants": "^0.3.0"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/markdown-remark": "6.0.0-beta.2",
    "@astrojs/mdx": "4.0.0-beta.3",
    "@astrojs/netlify": "6.0.0-beta.1",
    "@astrojs/partytown": "^2.1.2",
    "@astrojs/rss": "^4.0.9",
    "@astrojs/sitemap": "^3.2.1",
    "@biomejs/biome": "^1.9.4",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@expressive-code/plugin-collapsible-sections": "^0.38.2",
    "@expressive-code/plugin-line-numbers": "^0.38.2",
    "@fontsource-variable/cabin": "^5.1.0",
    "@fontsource-variable/source-code-pro": "^5.1.0",
    "@fontsource-variable/ubuntu-sans": "^5.1.0",
    "@iconify-json/fa": "^1.2.0",
    "@iconify-json/lucide": "^1.2.14",
    "@iconify-json/ri": "^1.2.3",
    "@kindspells/astro-shield": "^1.6.1",
    "@maxmmyron/rehype-section-headings": "^1.3.0",
    "@tailwindcss/postcss": "4.0.0-alpha.33",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/node": "^22.9.0",
    "@vite-pwa/assets-generator": "^0.2.6",
    "@vite-pwa/astro": "^0.4.3",
    "@vite-pwa/workbox-window": "^8.0.0",
    "astro": "5.0.0-beta.7",
    "astro-auto-import": "^0.4.4",
    "astro-expressive-code": "^0.38.2",
    "astro-font": "^0.1.81",
    "astro-icon": "^1.1.2",
    "lefthook": "^1.8.2",
    "marked": "^15.0.0",
    "mdast-util-to-string": "^4.0.0",
    "netlify-cli": "^17.37.2",
    "prettier": "^3.3.3",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-prisma": "^5.0.0",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "prisma": "^5.22.0",
    "reading-time": "^1.5.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-external-links": "^3.0.0",
    "remark-directive": "^3.0.0",
    "remark-emoji": "^5.0.1",
    "remark-toc": "^9.0.0",
    "satori": "^0.11.2",
    "satori-html": "^0.3.2",
    "schema-dts": "^1.1.2",
    "tailwindcss": "4.0.0-alpha.33",
    "typescript": "~5.6.3",
    "unist-util-visit": "^5.0.0",
    "vite-plugin-pwa": "^0.21.0"
  }
}
