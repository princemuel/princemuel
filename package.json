{
  "name": "princemuel.com",
  "type": "module",
  "version": "0.3.0",
  "description": "My personal website built with Astro, Qwik, Tailwind CSS, MDX, and the Github API",
  "private": true,
  "repository": "git@github.com:princemuel/princemuel.com.git",
  "author": "princemuel <vansomecsam@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "astro dev --open --host",
    "start": "astro dev",
    "build": "astro check && astro build",
    "build:dev": "astro check && astro build -- --force",
    "preview": "astro preview",
    "astro": "astro",
    "clean": "rimraf ./.astro ./dist ./.vercel",
    "typecheck": "tsc --noEmit",
    "fmt": "prettier --write .",
    "fmt:check": "prettier --check .",
    "fmt:lint": "oxlint -D correctness -D perf -A debugger -A eqeqeq --jsx-a11y-plugin --fix -c ./.eslintrc.json",
    "lint": "npm run typecheck && npm run fmt:lint && npm run fmt",
    "get:env": "npx vercel link && npx vercel env pull",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "prepare": "node .husky/install.mjs",
    "lint-staged": "lint-staged",
    "postinstall": "prisma generate"
  },
  "engines": {
    "node": "20.x"
  },
  "prisma": {
    "seed": "node ./prisma/seed.mjs"
  },
  "dependencies": {
    "@astrojs/check": "^0.6.0",
    "@astrojs/mdx": "^2.3.1",
    "@astrojs/rss": "^4.0.5",
    "@astrojs/sitemap": "^3.1.4",
    "@astrojs/tailwind": "^5.1.0",
    "@astrojs/vercel": "^7.5.4",
    "@builder.io/qwik": "^1.5.3",
    "@expressive-code/plugin-collapsible-sections": "^0.35.3",
    "@expressive-code/plugin-line-numbers": "^0.35.3",
    "@prisma/client": "^5.13.0",
    "@qwikdev/astro": "^0.5.15",
    "@qwikest/icons": "^0.0.13",
    "@t3-oss/env-core": "^0.10.1",
    "@upstash/ratelimit": "^1.1.3",
    "@upstash/redis": "^1.30.1",
    "@vercel/edge": "^1.1.1",
    "@vercel/og": "^0.6.2",
    "@vercel/speed-insights": "^1.0.10",
    "astro": "^4.7.1",
    "astro-expressive-code": "^0.35.3",
    "astro-font": "^0.0.80",
    "astro-htmx": "^1.0.5",
    "astro-icon": "^1.1.0",
    "htmx.org": "^1.9.12",
    "octokit": "^3.2.1",
    "openai": "^4.42.0",
    "outvariant": "^1.4.2",
    "resend": "^3.2.0",
    "sanitize-html": "^2.13.0",
    "sharp": "^0.33.3",
    "tailwind-variants": "^0.2.1",
    "typescript": "^5.4.5",
    "zod": "^3.23.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@iconify-json/lucide": "^1.1.187",
    "@tailwindcss/typography": "^0.5.13",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/sanitize-html": "^2.11.0",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "astro-eslint-parser": "^1.0.2",
    "css-has-pseudo": "^6.0.3",
    "eslint": "^8.57.0",
    "eslint-plugin-astro": "^1.1.2",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-oxlint": "^0.2.9",
    "eslint-plugin-qwik": "^1.5.3",
    "globals": "^15.1.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "marked": "^12.0.2",
    "mdast-util-to-string": "^4.0.0",
    "oxlint": "^0.3.2",
    "postcss-import": "^16.1.0",
    "postcss-preset-env": "^9.5.11",
    "prettier": "^3.2.5",
    "prettier-plugin-astro": "^0.13.0",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-prisma": "^5.0.0",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "prisma": "^5.13.0",
    "reading-time": "^1.5.0",
    "rehype-autolink-headings": "^7.1.0",
    "remark-emoji": "^4.0.1",
    "remark-toc": "^9.0.0",
    "schema-dts": "^1.1.2",
    "tailwind-scrollbar": "^3.1.0",
    "tailwindcss": "^3.4.3",
    "tailwindcss-animate": "^1.0.7",
    "unist-util-visit": "^5.0.0"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,astro}": "oxlint",
    "**/*.{md,astro}": [
      "astro check"
    ]
  }
}
